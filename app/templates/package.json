  "license": "MIT",
  <% if (global) { %>"preferGlobal": true,
  "bin": {
    "<%= name %>": "cli.js"
  },
  <% } if (test) { %>"scripts": {
    "test": "<%= test %>"
  },
  <% } %>"files": [
    <% if (global) { %>"cli.js"<% } else { %>"index.js"<% } %>
  ],
  "homepage": "https://github.com/eush77/<%= name %>",
  "repository": "eush77/<%= name %>",
  "bugs": {
    "url": "https://github.com/eush77/<%= name %>/issues"
  },
  "keywords": [<%=
   keywords
     .map(function (kw) {
       return '\n    "' + kw + '"';
     })
     .join(',') %>
  ],
  "dependencies": {},
  "devDependencies": {}
}
